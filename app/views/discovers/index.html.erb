<h1>Viewing Party</h1><br>

<h2>Discover Movies</h2><br>

<%= button_to "Find Top Rated Movies", user_movies_path(@user), method: :get %><br>

<%= form_with url: user_movies_path(@user), method: :get, local: true do |f| %>
  <%= f.label :search %>
  <%= f.text_field :search %>
  <%= f.submit "Find Movies" %>
<% end %>